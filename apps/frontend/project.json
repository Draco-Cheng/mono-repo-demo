{
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "cd apps/frontend && npm run build"
            }
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "cd apps/frontend && npm run dev"
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "cd apps/frontend && npm run test"
            }
        },
        "test:watch": {
            "executor": "nx:run-commands",
            "options": {
                "command": "cd apps/frontend && npm run test:watch"
            }
        },
        "test:coverage": {
            "executor": "nx:run-commands",
            "options": {
                "command": "cd apps/frontend && npm run test:coverage"
            }
        }
    }
}