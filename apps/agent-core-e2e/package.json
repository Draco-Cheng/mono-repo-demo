{
  "name": "@mono-repo-skeleton/agent-core-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@mono-repo-skeleton/agent-core"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/agent-core-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@mono-repo-skeleton/agent-core:build",
          "@mono-repo-skeleton/agent-core:serve"
        ]
      }
    }
  }
}
